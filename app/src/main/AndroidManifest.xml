<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.xiangweixin.myownstudy">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".camera.camera2.Camera2Activity"/>
        <activity android:name=".camera.camera1.Camera1Activity"/>
        <activity android:name=".ffmpeg.onlyPlayVideo.OnlyPlayVideoActivity"/>
        <activity android:name=".UtilActivity"/>
        <activity android:name=".audiorecord.AudioRecordActivity"/>
        <activity android:name=".opengl.OpenGLActivity"/>
        <activity android:name=".opengl.GLSurfaceViewActivity"/>
        <activity android:name=".SecondCameraActivity"/>
        <activity android:name=".FirstCameraActivity"/>
        <activity android:name=".opengl.BlendActivity"/>
        <activity android:name=".opengl.ImageActivity"/>
    </application>

</manifest>